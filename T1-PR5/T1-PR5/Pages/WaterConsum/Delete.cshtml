@page
@model T1_PR5.Pages.WaterConsum.DeleteModel
@{
	ViewData["Title"] = "Delete";
}

<h3>@ViewData["Title"]</h3>
<hr />

<div class="row shadow">
	<input type="hidden" asp-for="Consum.Id" />
	<div class="col-9">
		<div class="form-group">
			<label asp-for="Consum.Region" class="control-label"></label>
			<input asp-for="Consum.Region" class="form-control" readonly />
		</div>
		<div class="form-group">
			<label asp-for="Consum.City" class="control-label"></label>
			<input asp-for="Consum.City" class="form-control" readonly />
		</div>
		<div class="form-group">
			<label asp-for="Consum.Year" class="control-label"></label>
			<input asp-for="Consum.Year" class="form-control" readonly />
		</div>
		<div class="form-group">
			<label asp-for="Consum.Consum" class="control-label"></label>
			<input asp-for="Consum.Consum" class="form-control" readonly />
		</div>
		<form method="post">
			<div class="form-group mt-2">
				<input type="submit" value="Delete" class="btn btn-danger" />
				<a class="btn btn-secondary" asp-page="./Index">Back to List</a>
			</div>
		</form>
	</div>
</div>
